{
  "name": "x-code-inspect",
  "version": "1.0.0",
  "description": "This is a package for checking JavaScript projects. You only need to install this package to use detection tools such as ESLint. Most importantly, you don't need to do extra ESLint configuration, just use the built-in default configuration.",
  "main": "dist/index.js",
  "scripts": {
    "dist": "x-package-version-strict-check && webpack  --mode=production --progress && tsc --emitDeclarationOnly",
    "test": "node dist/index.js --path ./test",
    "cli": "npm unlink x-code-inspect && npm link x-code-inspect",
    "check": "x-code-inspect",
    "release": "standard-version && git push && git push --tags",
    "ct": "git add -A && git-cz -a"
  },
  "bin": {
    "x-code-inspect": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xucongli1989/x-code-inspect.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "code",
    "check",
    "eslint",
    "javascript"
  ],
  "author": "XCL",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/xucongli1989/x-code-inspect/issues"
  },
  "homepage": "https://github.com/xucongli1989/x-code-inspect",
  "dependencies": {
    "@typescript-eslint/parser": "2.9.0",
    "ansicolor": "1.1.92",
    "art-template": "4.13.2",
    "as-table": "1.0.55",
    "babel-eslint": "10.0.3",
    "caller-path": "3.0.0",
    "cfonts": "2.4.5",
    "cheerio": "1.0.0-rc.3",
    "commander": "4.0.1",
    "del": "5.1.0",
    "eslint": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "18.1.5",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "make-dir": "3.0.0",
    "ololog": "1.1.149",
    "prettier": "1.19.1",
    "prettyoutput": "1.2.0",
    "shelljs": "0.8.3",
    "sync-request": "6.1.0",
    "typescript": "3.7.2",
    "update-notifier": "3.0.1",
    "x-js-kit": "4.5.0",
    "x-package-version-strict-check": "1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "7.7.4",
    "@babel/core": "7.7.4",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "7.7.4",
    "@babel/plugin-transform-async-to-generator": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/preset-typescript": "7.7.4",
    "@types/cheerio": "0.22.18",
    "@types/commander": "2.12.2",
    "@types/eslint": "6.1.3",
    "@types/prettier": "1.19.0",
    "@types/shelljs": "0.8.6",
    "@types/update-notifier": "2.5.0",
    "art-template-loader": "1.4.3",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "commitizen": "4.0.3",
    "event-hooks-webpack-plugin": "2.1.4",
    "standard-version": "7.0.1",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2"
  },
  "files": [
    "dist",
    "bin",
    "doc"
  ],
  "engines": {
    "node": ">=10.13.0"
  }
}
